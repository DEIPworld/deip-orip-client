    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "inquirer": "^7.0.6",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-scss": "^2.0.0",
    "postcss-url": "^8.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "symlink-dir": "^3.1.2",
    "vue-template-compiler": "^2.6.10",
    "yargs": "^15.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "import/prefer-default-export": "off",
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true,
          "enforceInMethodNames": false
        }
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error",
            "info"
          ]
        }
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "vue/script-indent": [
        "error",
        2,
        {
          "baseIndent": 1
        }
      ],
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 3
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "*.vue"
        ],
        "rules": {
          "indent": "off"
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "autoprefixer": {}
    }
  }
}

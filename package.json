  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "vuetify"
    ],
    "rules": {
      "vuetify/no-deprecated-classes": "error",
      "vuetify/grid-unknown-attributes": "error",
      "vuetify/no-legacy-grid": "error",
      "import/prefer-default-export": "off",
      "no-param-reassign": [
        "error",
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "state",
            "Vue",
            "_Vue"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never",
          "vue": "never"
        }
      ],
      "no-restricted-syntax": "off",
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true,
          "enforceInMethodNames": false
        }
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error",
            "info"
          ]
        }
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "vue/script-indent": [
        "error",
        2,
        {
          "baseIndent": 1
        }
      ],
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 3
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "*.vue"
        ],
        "rules": {
          "indent": "off"
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
